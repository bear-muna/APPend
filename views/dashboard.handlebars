<div class="dashboard row justify-content-center">
    <div class="dashboard-photo">
        <!--
        <img src="{{profile.picture}} ???????" alt="profile picture">
        -->
    </div>
    <div class="dashboard-name">
        <h1>{{user.first_name}} {{user.last_name}}</h1>
        <h3>{{user.Profile.location}}</h3>
    </div>
    <div class="bio">
        <p>{{user.Profile.bio}}</p>
    </div>
    <div class="favorite-cuisines">
        <ul>
            {{#each user.Cuisine}}
            <li><button class="cuisine">{{name}}</button></li>
            {{/each}}
        </ul>
    </div>
    <div class="availability">
        <p>{{user.Profile.availability}}</p>
    </div>
    <div class="messages">
        <button class="btn message-btn">Messages</button>
    </div>
    <button class="settings-btn">Settings</button>
</div>